{
  "name": "my-simple-node-typescript",
  "version": "1.0.0",
  "description": "An amazing starter kit for using typescript with node",
  "main": "index.js",
  "scripts": {
    "prestart": "./node_modules/.bin/tsc index.ts",
    "start": "node index.js",
    "tsc:watch": "./node_modules/.bin/tsc -w index.ts",
    "compile": "./node_modules/.bin/tsc index.ts",
    "serve": "./node_modules/.bin/nodemon index.js",
    "pretest": "./node_modules/.bin/tsc test/*.spec.ts",
    "test": "./node_modules/.bin/mocha"
  },
  "author": "Jean-Yves DELMOTTE <delmotte.jy@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/lodash": "^4.14.96",
    "@types/mocha": "^2.2.47",
    "assert": "^1.4.1",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11",
    "typescript": "^2.6.2"
  }
}
